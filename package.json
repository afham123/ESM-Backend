{
  "name": "server",
  "version": "1.0.0",
  "description": "backend server for mv dictiory",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --watchAll --no-cache --verbose --silent=false",
    "pre-build": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc",
    "build-watch": "npm run clean && tsc -w",
    "pre-start": "npm run build",
    "start": "npm run build && node ./dist",
    "udp-start": "node ./dist/udpServer.js",
    "dev-start": "concurrently --kill-others \"npm run build-watch\" \"nodemon ./dist\"",
    "serve": "lt --port 5000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "googleapis": "^144.0.0",
    "graphql": "^16.9.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.2",
    "mongoose-paginate-v2": "^1.8.5",
    "nodemailer": "^6.9.15",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.7",
    "@types/nodemailer": "^6.4.16",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
